{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": [
      "enabled",
      {
        "ignore": "ignore",
        "customValidators": "customValidator",
        "skipUndeclared": "skipUndeclared"
      }
    ],
    "quotes": "error"
  }
}
